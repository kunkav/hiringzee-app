{
  "expo": {
    "name": "Hiringzee",
    "slug": "hiringzee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "android": {
      "splash": {
        "mdpi": "./assets/splash/splash.png",
        "hdpi": "./assets/splash/splash.png",
        "xhdpi": "./assets/splash/splash.png",
        "xxhdpi": "./assets/splash/splash.png",
        "xxxhdpi": "./assets/splash/splash.png",
        "resizeMode": "cover"
      }
    },
    "ios": {
      "splash": {
        "image": "./assets/img/splash/splash.png",
        "resizeMode": "cover"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
